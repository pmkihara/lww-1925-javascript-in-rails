<p style="color: green"><%= notice %></p>

<%= render 'monument_info', monument_local: @monument %>

<div>
  <%= link_to "Edit this monument", edit_monument_path(@monument) %> |
  <%= link_to "Back to monuments", monuments_path %>

  <%= button_to "Destroy this monument", monument_path(@monument), method: :delete %>
</div>
